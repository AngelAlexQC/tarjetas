{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EAS_BUILD": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EAS_BUILD": "true",
        "APP_NAME": "Tarjetas",
        "APP_SLUG": "tarjetas",
        "APP_BUNDLE_ID_IOS": "com.libelulasoft.tarjetas",
        "APP_BUNDLE_ID_ANDROID": "com.libelulasoft.tarjetas",
        "APP_SCHEME": "tarjetas"
      }
    },
    "bpichincha-production": {
      "extends": "production",
      "env": {
        "APP_NAME": "Banco Pichincha",
        "APP_SLUG": "banco-pichincha",
        "APP_BUNDLE_ID_IOS": "ec.com.pichincha.tarjetas",
        "APP_BUNDLE_ID_ANDROID": "ec.com.pichincha.tarjetas",
        "APP_SCHEME": "banco-pichincha",
        "TENANT_ID": "bpichincha"
      }
    },
    "bancolombia-production": {
      "extends": "production",
      "env": {
        "APP_NAME": "Bancolombia",
        "APP_SLUG": "bancolombia",
        "APP_BUNDLE_ID_IOS": "co.com.bancolombia.tarjetas",
        "APP_BUNDLE_ID_ANDROID": "co.com.bancolombia.tarjetas",
        "APP_SCHEME": "bancolombia",
        "TENANT_ID": "bancolombia"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
